{
    "name": "kanekescom/laravel-siasn-referensi",
    "description": "Library for reference in SIASN BKN",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Achmad Hadi Kurnia",
            "email": "imachmadhadikurnia@gmail.com",
            "homepage": "https://achmadhadikurnia.com"
        }
    ],
    "homepage": "https://github.com/kanekescom/laravel-siasn-referensi",
    "keywords": [
        "laravel",
        "siasn",
        "laravel-siasn-reference",
        "reference",
        "referensi"
    ],
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0",
        "prettus/l5-repository": "^2.9",
        "league/fractal": "^0.19.2",
        "league/csv": "^9.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.10",
        "orchestra/testbench": "^6.28.0"
    },
    "autoload": {
        "psr-4": {
            "Kanekescom\\SiasnReferensi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kanekescom\\SiasnReferensi\\Tests\\": "tests"
        }
    },
    "suggest": {
        "kanekescom/laravel-siasn": "Required for importing some data from SIASN API",
        "kanekescom/laravel-repository-enhancer": "Required for repository enhancer",
        "kanekescom/siasn-datasets": "Required for importing some data from CSV"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kanekescom\\SiasnReferensi\\SiasnReferensiServiceProvider"
            ]
        }
    }
}
